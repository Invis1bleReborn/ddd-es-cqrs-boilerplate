{
    "name": "invis1ble/ddd-es-cqrs-boilerplate",
    "description": "DDD ES CQRS Boilerplate",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "~7.4.0",
        "ext-amqp": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "api-platform/api-pack": "^1.3",
        "beberlei/assert": "^3.2",
        "broadway/broadway": "^2.3",
        "broadway/broadway-bundle": "^0.5.0",
        "broadway/event-store-dbal": "^0.5.0",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "gesdinet/jwt-refresh-token-bundle": "dev-fix-200",
        "lexik/jwt-authentication-bundle": "^2.8",
        "myclabs/php-enum": "^1.7",
        "nelmio/cors-bundle": "^2.1",
        "ramsey/uuid": "^4.0",
        "ramsey/uuid-doctrine": "^1.6",
        "symfony/amqp-messenger": "5.1.*",
        "symfony/apache-pack": "^1.0",
        "symfony/console": "5.1.*",
        "symfony/doctrine-messenger": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.1.*",
        "symfony/mercure-bundle": "^0.2.5",
        "symfony/messenger": "5.1.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/yaml": "5.1.*"
    },
    "require-dev": {
        "api-platform/schema-generator": "^2.2",
        "escapestudios/symfony2-coding-standard": "^3.11",
        "phpmd/phpmd": "^2.8",
        "phpunit/phpunit": "^9.3",
        "symfony/browser-kit": "^5.1",
        "symfony/debug-pack": "^1.0",
        "symfony/phpunit-bridge": "^5.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Invis1bleReborn/JWTRefreshTokenBundle"
        }
    ],
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Common\\": "src/Common/",
            "IdentityAccess\\": "src/IdentityAccess/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Common\\": "tests/Common/",
            "IdentityAccess\\": "tests/IdentityAccess/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "doctrine/doctrine-bundle": ">=2.1"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    }
}
